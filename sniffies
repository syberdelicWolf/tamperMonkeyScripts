// ==UserScript==
// @name         Sniffies Improved Colors
// @namespace    http://sniffies.com/
// @version      0.1
// @description  Updates the colors on the Sniffies website for improved visibility
// @author       Sygic Sithier
// @match        https://sniffies.com/*
// @icon         data:image/jpeg;base64,
// @grant        none
// ==/UserScript==

(function () {
  const originalFetch = window.fetch;
  const colorMap = {
    background: "#053642",
    waterway: "#05ADCC",
    water: "#05B3D3",
    park_outline: "#0D8CA5",
    landcover_wood: "#4D4D4D",
    "landuse-residential": "#8C6B49",
    building: "#264664",
    landcover_grass: "#005959",
    landuse_overlay_national_park: "#005555",
    park: "#4D6666",
    tunnel_motorway_casing: "#333333",
    tunnel_motorway_inner: "#666666",
    road_area_pier: "#999999",
    road_pier: "#CCCCCC",
    highway_path: "#0D1E2F",
    highway_minor: "#0D1E2F",
    highway_motorway_casing: "#3D5473",
    highway_motorway_inner: "#3D5473",
    highway_motorway_subtle: "#3D5473",
    highway_major_casing: "#1D3051",
    highway_major_inner: "#2D4262",
    highway_major_subtle: "#3D5473",
    highway_motorway_bridge_casing: "#4D6483",
    highway_motorway_bridge_inner: "#4D6483",
    "landcover-ice-shelf": "#19A2EB",
    "landcover-glacier": "#91CFF1",
    "aeroway-area": "#061221",
    "aeroway-taxiway": "#0b1f2d",
    "aeroway-runway": "#0b1f2d",
    highway_motorway_bridge_casing: "#0b1f2d",
  };

  const paintProperties = ["fill-color", "background-color", "line-color"];

  async function fetchAndModifyMapStyles(url) {
    const response = await originalFetch(url);
    const mapStyleJson = await response.json();
    const { layers } = mapStyleJson;
    const unmodifedPaints = [];

    layers.forEach((layer) => {
      if (!layer.id || !layer.paint) return;
      let foundMatch = false;
      for (const paintProp of paintProperties) {
        if (layer.paint[paintProp]) {
          layer.paint[paintProp] = colorMap[layer.id];
          foundMatch = true;
          break;
        }
      }
      if (!foundMatch) {
        unmodifedPaints.push(layer.id);
      }
    });

    console.log(
      `The following layers were not modified: ${unmodifedPaints.join(",")}`
    );

    const mapStyleJsonString = JSON.stringify(mapStyleJson);
    const mapStyleJsonBlob = new Blob([mapStyleJsonString], {
      type: "application/json",
    });

    return new Response(mapStyleJsonBlob, {
      headers: { "Content-Type": "application/json" },
    });
  }

  window.fetch = (url, options) => {
    if (!url.url.includes("mapboxMapStyles")){
      return originalFetch(url, options);
    }
    return fetchAndModifyMapStyles(url);
  };
})();
